*
    box-sizing: border-box;
body
    margin: 0;
    background-color: #f5f6fa
    font-size: 14px;
    font-family: 'Roboto', sans-serif;
    overflow hidden;

h2,h3,h4,h5,ul,body,html
    padding: 0px;
    margin: 0px;

.v-header
    height: 100vh;
    display: flex;

.container
    max-width: 49rem;
    padding-left: 1rem;
    padding-right: 1rem;
    margin: auto;

.fullscreen-video-wrap
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100vh;
    overflow: hidden;

.fullscreen-video-wrap video 
    min-width: 100%;
    min-height: 100%;

.header-overlay
    height: 100vh;
    width: 100vw;
    position: absolute;
    top: 0;
    left: 0;
    background: #8998AC
    opacity: .30;
    
.mtop16
    margin-top: 16px;


.wrapper
    min-height: 100vh
    overflow hidden
    position: absolute;
    top: 0;
    left: 0;
    .col1
        float left
        position rela
        z-index: 999;         
        width: 17%;     
    .col2
      float left
      width: 83%;

.sidebar
    background-color: #000000
    min-height: 100vh
    z-index: 1;
    opacity: .70;
    .section-top
        background-color: #f5f500
        box-shadow: 0 0.1rem 1rem rgb(0,0,0 0.15 )
    .logo
        display: block
        margin: opx auto
        padding-top: 24px;
        width: 90%;
    .user
        color: #000000 !important
        padding: 16px;
        a 
            color: #71258a
            font-size: 1.5em;
    .main
        display: block
        margin-top: 8px
        ul
            li
            margin-bottom: 4px;
            
            a 
                color: #8998AC !important
                display: block
                font-size: 1.2em;
                padding: 12px 16px
                i
                    margin-right: 8px;
                &:hover
                    background-color: #f5f500;
                    box-shadow: 0 0.1rem 1rem  rgb(0,0,0 0.15);
                    color: #000000 !important;
                    text-decoration: none;

.navbar
    background-color: #f5f500;
    width: 100vw;
    opacity: .70;
    a
        color: #000000;

.page
    padding-top: 16px;
    padding-bottom: 16px;
    .breadcrumb
        background-color: #000000;
        box-shadow: 0 0.1rem 1rem  rgb(0,0,0 0.10) !important;
        opacity: 50%
        .breadcrumb-item
            a
                color: #f5f500 
                &:hover
                    text-decoration: none !important;
    .panel
        background-color: #000000;
        border-radius: 8px;    
        border-bottom: 3px solid #f5f500;
        width: 100%;
        opacity: 70%;
        
        .header
            border-bottom: 1px solid #f5f500;
            h2.title
                diplay inline-block;
                color: #71258a;
                font-size: 1.5em;
                padding: 12px 16px; 
        .insider
            padding: 16px;
            color: #8998AC;
        .table
            color: #71258a;
        .table1
            color: #f5f500;
                   

.opts
    a
        border-radius: 4px;
        border: 1px solid #f5f500
        display: inline-block;
        padding: 8px 12px;
        color: #71258a;

.page_user
    .mini_profile
        .avatar
            display: block;
            margin: 0px auto;
            border-radius: 50%;
            width: 120px;
        .info
            color: #8998Ac;
            span
                display: block;
                font-size: 1.1em;
                width: 100%;
            span.title
                color: #f5f500;  
                margin-top: 16px;
        .btn
            width: 100%;
            margin-top: 16px;
            background: #f5f500
            color: #000000
.form
    .title
        border-radius: 4px;
        display: inline-block;
        padding: 8px 12px;
        color: #f5f500;
        top: 5px;
        right: 5px;  
    .btn1
        width: 20%;
        margin-top: 16px;
        border: solid  3px;
        background: #f5f500;
        color: #000000          

            
        
             
                        








        



